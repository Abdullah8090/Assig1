    "# find Gravitational Force\n",
    "def force(M, m, r):\n",
    "    F = (G*M*m)/(r**2)\n",
    "    \n",
    "    return round(F, 2) \n",
    "  \n",
    "G = 6.67*(10**-11)\n",
    "M = 6.0*10**24\n",
    "m = 7.34*10**22\n",
    "r = 3.84*10**8\n",
    "print(\"The Gravitational Force is: \", force(M, m, r))"
   ]
  }
